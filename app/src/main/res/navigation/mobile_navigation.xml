<?xml version="1.0" encoding="utf-8"?>
<navigation xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:id="@+id/mobile_navigation"
    app:startDestination="@+id/view">

    <fragment
        android:id="@+id/view"
        android:name="com.example.passwordsaving.ui.ViewFragment"
        android:label="@string/View"
        tools:layout="@layout/fragment_view">
    <action
        android:id="@+id/action_viewFragment_to_addFragment"
        app:destination="@id/add" />
    </fragment>

    <fragment
        android:id="@+id/add"
        android:name="com.example.passwordsaving.ui.AddFragment"
        android:label="@string/Add"
        tools:layout="@layout/fragment_add" />
</navigation>